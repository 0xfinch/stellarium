#fly to the Europa
# move to iss
date utc 2007:08:04T10:00:00
timerate rate 0.001
landscape action load name "iss" type spherical maptex "..\..\textures\landscapes\iss.png"
zoom fov 240 duration 3
wait duration 3
flag landscape 1
wait duration 10.0
flag landscape 0
zoom fov 179.9 duration 3
wait duration 3

select planet Europa pointer 0
flag track_object 1
set center_fov_ratio -0.33333
wait duration 12
set desired_angle_of_view_selected_object 1.0
set number_of_flying_steps 1200
wait duration 2
set number_of_flying_steps 1000
wait duration 2
set number_of_flying_steps 800
wait duration 2
set number_of_flying_steps 600
wait duration 20
set desired_angle_of_view_selected_object 1.5707
wait duration 18
deselect

select planet Mars pointer 0
flag track_object 1
set center_fov_ratio -0.33333
wait duration 12
set desired_angle_of_view_selected_object 0.30
set number_of_flying_steps 1000
wait duration 25
deselect

//Move the observer to Mars
zoom auto out duration 3
wait duration 3
set home_planet "Mars"
zoom fov 240 duration 3
wait duration 3
landscape action load name "Husband Hill" type spherical maptex "..\..\textures\landscapes\husband-hill.png"
flag landscape 1
wait duration 10.0
flag landscape 0
zoom fov 179.9 duration 3
wait duration 3

//Move the observer to Earth
set home_planet "Earth"
zoom fov 240 duration 3
wait duration 3
landscape action load name "everest" type spherical maptex "..\..\textures\landscapes\everest.png"
flag landscape 1
flag hyper_stars 0
flag atmosphere 1
timerate rate 2000
wait duration 20
timerate rate 1
flag atmosphere 0
flag hyper_stars 1
meteors zhr 144000
wait duration 30.0
meteors zhr 10
wait duration 10.0

script action end